{
    "name": "KsMobil",
    "displayName" : "Klarschiff – Bürgerbeteiligung",
    "buildVersion": "0.4",
    "mode": "debug",
    "debugLevel": 1,
    "eliminate": false,
    "minify": false,

    "application": {
        "displayName": "Klarschiff – Bürgerbeteiligung",
        "baseUrl": null,
        "version": "0.4",
        "debugMode": true,
        "useTransitions": true,
        "enableTouchOverflow": true,
        "keyPrefix": "#m#",
        "keySuffix": "_",
        "timeStampCreated": "created_at",
        "timeStampUpdated": "updated_at",
        "m_id": "m_id"
    },

    "proxies": [{
        "baseUrl": "http://demo.klarschiff-hro.de/geodienste/klarschiff/wfs",
