    },{ 
        "baseUrl": "http://demo.klarschiff-hro.de",
        "proxyAlias": "klarschiff_desktop"
    }],

    "libraries": [{
        "name": "jquery-tmpl",
        "refs": ["jquery.tmpl.min.js"]
    },{
        "name": "jquery-form",
        "refs": ["jquery.form.min.js"]
    },{
        "name": "proj4js",
        "refs": ["proj4js-combined.js"]
    },{
        "name": "OpenLayers",
        "refs": ["OpenLayers.js"]
    }],

    "htmlHeader": [
        "<meta charset=\"utf-8\">",
